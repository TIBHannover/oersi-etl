<lesson_plan> a skos:Concept;
          skos:topConceptOf <scheme> ;
          skos:prefLabel "Unterrichtsplanung"@de, "Lesson Plan"@en, "План уроку"@uk .

<map> a skos:Concept;
          skos:topConceptOf <scheme> ;
          skos:prefLabel "Karte"@de, "Map"@en, "Мапа"@uk .

