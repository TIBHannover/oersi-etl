    dct:title "European Framework for the Digital Competence of Educators"@en;
    dct:title "Europäischer Rahmen für die Digitale Kompetenz Lehrender"@de ;
    dct:created "2023-03-16"^^xsd:date;
    dct:license <http://creativecommons.org/publicdomain/zero/1.0/> ;
    skos:hasTopConcept <1>, <2>, <3>, <4>, <5>, <6> .

<1> a skos:Concept ;
    skos:prefLabel "Professional Engagement"@en ;
    skos:prefLabel "Berufliches Engagement"@de ;
    skos:topConceptOf <> ;
    skos:exactMatch comeIn:1 ;
    skos:narrower <1.1>, <1.2>, <1.3>, <1.4> .

<2> a skos:Concept ;
    skos:prefLabel "Digital Resources"@en ;
    skos:prefLabel "Digitale Ressourcen"@de ;
        skos:topConceptOf <> ;
    skos:exactMatch comeIn:2 ;
    skos:narrower <2.1>, <2.2>, <2.3> .

<3> a skos:Concept ;
    skos:prefLabel "Teaching and Learning"@en ;
    skos:prefLabel "Lehren und Lernen"@de ;
    skos:topConceptOf <> ;
    skos:exactMatch comeIn:3 ;
    skos:narrower <3.1>, <3.2>, <3.3>, <3.4> .

<4> a skos:Concept ;
    skos:prefLabel "Assessment"@en ;
    skos:prefLabel "Evaluation"@de ;
    skos:topConceptOf <> ;
    skos:exactMatch comeIn:4 ;
    skos:narrower <4.1>, <4.2>, <4.3> .

<5> a skos:Concept ;
    skos:prefLabel "Empowering Learners"@en ;
    skos:prefLabel "Lernerorientierung"@de ;
    skos:topConceptOf <> ;
    skos:exactMatch comeIn:5 ;
    skos:narrower <5.1>, <5.2>, <5.3> .

<6> a skos:Concept ;
    skos:prefLabel "Facilitating Learner's Digital Competence"@en ;
    skos:prefLabel "Förderung der digitalen Kompetenz der Lernenden"@de ;
    skos:topConceptOf <> ;
    skos:exactMatch comeIn:6 ;
    skos:narrower <6.1>, <6.2>, <6.3>, <6.4>, <6.5> .

<1.1> a skos:Concept ;
    skos:prefLabel "Organisational communication"@en ;
    skos:prefLabel "Berufliche Kommunikation"@de ;
    skos:definition "To use digital technologies to enhance organisational communication with learners, parents and third parties. To contribute to collaboratively developing and improving organisational communication strategies."@en ;
    skos:definition "Nutzung von digitalen Medien zur Verbesserung der organisatorischen Kommunikation mit Lernenden, Eltern und Dritten; Gemeinsame Entwicklung und Verbesserung organisatorischer Kommunikationsstrategien."@de ;
    skos:broader <1> ;
    skos:exactMatch comeIn:1.1 ;
    skos:inScheme <> .

<1.2> a skos:Concept ;
    skos:prefLabel "Professional collaboration"@en ;
    skos:prefLabel "Berufliche Zusammenarbeit"@de;
    skos:definition "To use digital technologies to engage in collaboration with other educators, sharing and exchanging knowledge and experiences and collaboratively innovating pedagogic practices."@en ;
    skos:definition "Nutzung von digitalen Medien zur Zusammenarbeit mit anderen Lehrenden, zum Austausch von Wissen und Erfahrungen und zur Erneuerung von gemeinsamen pädagogischen Praktiken."@de ;
    skos:broader <1> ;
    skos:exactMatch comeIn:1.2 ;
    skos:inScheme <> .

<1.3> a skos:Concept ;
    skos:prefLabel "Reflective practice"@en ;
    skos:prefLabel "Reflektierte Praxis"@de ;
    skos:definition "To individually and collectively reflect on, critically assess and actively develop one’s own digital pedagogical practice and that of one’s educational community."@en ;
    skos:definition "Die eigene digitale pädagogische Praxis sowie die der Bildungsgemeinschaft individuell und gemeinsam reflektieren, selbstkritisch beurteilen und aktiv entwickeln."@de ;
    skos:broader <1> ;
    skos:exactMatch comeIn:1.3 ;
    skos:inScheme <> .

<1.4> a skos:Concept ;
    skos:prefLabel "Digital CPD"@en ;
    skos:prefLabel "Digitale Weiterbildung"@de ;
    skos:definition "To use digital sources and resources for continuous professional development."@en ;
    skos:definition "Nutzung von digitalen Quellen und Ressourcen für die fortlaufende Weiterbildung."@de ;
    skos:broader <1> ;
    skos:exactMatch comeIn:1.4 ;
    skos:inScheme <> .

<2.1> a skos:Concept ;
    skos:prefLabel "Selecting"@en ;
    skos:prefLabel "Auswählen"@de ;
    skos:definition "To identify, assess and select digital resources for teaching and learning. To consider the specific learning objective, context, pedagogical approach, and learner group, when selecting digital resources and planning their use."@en ;
    skos:definition "Auswahl, Identifizierung und Auswertung digitaler Ressourcen für das Lehren und Lernen; Berücksichtigung von konkretem Lernziel, Kontext, didaktischem Ansatz und der Lerngruppe bei der Auswahl digitaler Ressourcen und der Planung ihrer Nutzung."@de ;
    skos:broader <2> ;
    skos:exactMatch comeIn:2.1 ;
    skos:inScheme <> .

<2.2> a skos:Concept ;
    skos:prefLabel "Creating & modifying"@en ;
    skos:prefLabel "Erstellen und Anpassen"@de;
    skos:definition "To modify and build on existing openly-licensed resources and other resources where this is permitted. To create or co-create new digital educational resources. To consider the specific learning objective, context, pedagogical approach, and learner group, when designing digital resources and planning their use."@en ;
    skos:definition "Modifizierung und Weiterentwicklung bestehender Ressourcen, sofern rechtlich möglich; Erstellung oder Mitgestaltung von neuen digitalen Bildungsressourcen; Berücksichtigung von konkretem Lernziel, Kontext, didaktischem Ansatz und der Lerngruppe beim Entwurf digitaler Ressourcen und der Planung ihrer Nutzung."@de ;
    skos:broader <2> ;
    skos:exactMatch comeIn:2.2 ;
    skos:inScheme <> .

<2.3> a skos:Concept ;
    skos:prefLabel "Managing, protecting, sharing"@en ;
    skos:prefLabel "Organisieren, Schützen, und Teilen"@de ;
    skos:definition "To organise digital content and make it available to learners, parents and other educators. To effectively protect sensitive digital content. To respect and correctly apply privacy and copyright rules. To understand the use and creation of open licenses and open educational resources, including their proper attribution."@en ;
    skos:definition "Digitale Inhalte organisieren und Lernenden, Eltern und anderen Lehrenden zur Verfügung stellen; Sensible digitale Inhalte effektiv schützen; Datenschutz- und Urheberrechtsbestimmungen respektieren und korrekt anwenden; Die Verwendung und Erstellung von offenen Lizenzen und offenen Bildungsressourcen einschließlich ihrer angemessenen Zuordnung verstehen."@de ;
    skos:broader <2> ;
    skos:exactMatch comeIn:2.3 ;
    skos:inScheme <> .

<3.1> a skos:Concept ;
    skos:prefLabel "Teaching"@en ;
    skos:prefLabel "Lehren"@de ;
    skos:definition "To plan for and implement digital devices and resources in the teaching process, so as to enhance the effectiveness of teaching interventions. To appropriately manage and orchestrate digital teaching interventions. To experiment with and develop new formats and pedagogical methods for instruction."@en ;
    skos:definition "Einsatzplanung und -gestaltung von digitalen Geräten und Materialien im Unterricht zur Verbesserung der Effektivität von Lehrinterventionen; digitale Unterrichtsmethoden angemessen organisieren und gestalten; neue Formate und pädagogische Methoden für den Unterricht entwickeln und ausprobieren."@de ;
    skos:broader <3> ;
    skos:exactMatch comeIn:3.1 ;
    skos:inScheme <> .

<3.2> a skos:Concept ;
    skos:prefLabel "Guidance"@en ;
    skos:prefLabel "Lernbegleitung"@de;
    skos:definition "To use digital technologies and services to enhance the interaction with learners, individually and collectively, within and outside the learning session. To use digital technologies to offer timely and targeted guidance and assistance. To experiment with and develop new forms and formats for offering guidance and support."@en ;
    skos:definition "Nutzung von digitalen Medien und Diensten zwecks Verbesserung der Interaktion mit den Lernenden auf individueller Ebene und als Gruppe innerhalb und außerhalb des Unterrichts; Nutzung von digitalen Medien, um rechtzeitig und gezielt Beratung und Unterstützung anbieten zu können; neue Formen und Formate der Hilfestellung und Anleitung entwickeln und experimentell einsetzen."@de ;
    skos:broader <3> ;
    skos:exactMatch comeIn:3.2 ;
    skos:inScheme <> .

<3.3> a skos:Concept ;
    skos:prefLabel "Collaborative learning"@en ;
    skos:prefLabel "Kollaboratives Lernen"@de ;
    skos:definition "To use digital technologies to foster and enhance learner collaboration. To enable learners to use digital technologies as part of collaborative assignments, as a means of enhancing communication, collaboration and collaborative knowledge creation."@en ;
    skos:definition "Nutzung von digitalen Medien zur Förderung und Verbesserung von gemeinsamen Lernstrategien; Lernende befähigen, digitale Medien in Gruppen- und Teamarbeit zu nutzen, um Kommunikation innerhalb der Lerngruppe, Zusammenarbeit und gemeinsame Wissensgenerierung zu verbessern."@de ;
    skos:broader <3> ;
    skos:exactMatch comeIn:3.3 ;
    skos:inScheme <> .

<3.4> a skos:Concept ;
    skos:prefLabel "Self-regulated learning"@en ;
    skos:prefLabel "Selbstgesteuertes Lernen"@de ;
    skos:definition "To use digital technologies to support self-regulated learning processes, i.e. to enable learners to plan, monitor and reflect on their own learning, provide evidence of progress, share insights and come up with creative solutions."@en ;
    skos:definition "Nutzung von digitalen Medien zur Unterstützung von selbstgesteuerten Lernprozessen, d. h. den Lernenden sollte ermöglicht werden, ihr eigenes Lernen zu planen, zu überprüfen und zu reflektieren, Fortschritte zu dokumentieren, Ergebnisse zu kommunizieren und kreative Lösungen zu erarbeiten."@de ;
    skos:broader <3> ;
    skos:exactMatch comeIn:3.4 ;
    skos:inScheme <> .

<4.1> a skos:Concept ;
    skos:prefLabel "Assessment strategies"@en ;
    skos:prefLabel "Lernstand erheben"@de ;
    skos:definition "To use digital technologies for formative and summative assessment. To enhance the diversity and suitability of assessment formats and approaches."@en ;
    skos:definition "Verwendung von digitalen Medien zur Lernkontrolle und Leistungsbeurteilung; Verbesserung der Vielfalt und der Angemessenheit von Beurteilungsformaten und -ansätzen."@de ;
    skos:broader <4> ;
    skos:exactMatch comeIn:4.1 ;
    skos:inScheme <> .

<4.2> a skos:Concept ;
    skos:prefLabel "Analysing evidence"@en ;
    skos:prefLabel "Lern-Evidenz analysieren"@de;
    skos:definition "To generate, select, critically analyse and interpret digital evidence on learner activity, performance and progress, in order to inform teaching and learning."@en ;
    skos:definition "Digitale Informationen zu Lernerverhalten, Leistung und Fortschritt erheben, auswählen, kritisch analysieren und interpretieren, um Rückschlüsse für den Unterricht zu ziehen."@de ;
    skos:broader <4> ;
    skos:exactMatch comeIn:4.2 ;
    skos:inScheme <> .

<4.3> a skos:Concept ;
    skos:prefLabel "Feedback & planning"@en ;
    skos:prefLabel "Feedback und Planung"@de ;
    skos:definition "To use digital technologies to  provide targeted and timely feedback to learners. To adapt teaching strategies and to provide targeted support, based on the evidence generated by the digital technologies used. To enable learners and parents to understand the evidence provided by digital technologies and use it for decision-making."@en ;
    skos:definition "Nutzung von digitalen Medien, um Lernenden gezielt und zeitnah Feedback zu geben; Anpassung von Unterrichtsstrategien und gezielte Unterstützung von Lernenden anhand verfügbarer digitaler Informationen; Lernenden und Eltern ermöglichen, digitale Informationen zu verstehen und sie für die Entscheidungsfindung zu nutzen."@de ;
    skos:broader <4> ;
    skos:exactMatch comeIn:4.3 ;
    skos:inScheme <> .

<5.1> a skos:Concept ;
    skos:prefLabel "Accessibility & inclusion"@en ;
    skos:prefLabel "Digitale Teilhabe"@de ;
    skos:definition "To ensure accessibility to learning resources and activities, for all learners, including those with special needs. To consider and respond to learners’ (digital) expectations, abilities, uses and misconceptions, as well as contextual, physical or cognitive constraints to their use of digital technologies."@en ;
    skos:definition "Gewährleisten, dass alle Lernenden, auch solche mit besonderen Bedürfnissen, Zugang zu den Lernressourcen und -aktivitäten haben; (Digitale) Erwartungen, Fähigkeiten, Vorkenntnisse und Missverständnisse der Lernenden berücksichtigen sowie kontextbezogene, physische oder kognitive Einschränkungen bei der Mediennutzung bedenken."@de ;
    skos:broader <5> ;
    skos:exactMatch comeIn:5.1 ;
    skos:inScheme <> .

<5.2> a skos:Concept ;
    skos:prefLabel "Differentation & personalisation"@en ;
    skos:prefLabel "Differenzierung und Individualisierung"@de;
    skos:definition "To use digital technologies to address learners’ diverse learning needs, by allowing learners to advance at different levels and speeds, and to follow individual learning pathways and objectives."@en ;
    skos:definition "Nutzung von digitalen Medien, um unterschiedlichen Lernbedürfnissen der Lernenden gerecht zu werden, indem ihnen ermöglicht wird, in den verschiedenen Niveaus und der Lerngeschwindigkeit fortzuschreiten, ihr individuelles Lernziel zu erreichen und individuelle Lernwege zu beschreiten."@de ;
    skos:broader <5> ;
    skos:exactMatch comeIn:5.2 ;
    skos:inScheme <> .

<5.3> a skos:Concept ;
    skos:prefLabel "Actively engaging learners"@en ;
    skos:prefLabel "Aktive Einbindung der Lernenden"@de ;
    skos:definition "To use digital technologies to foster learners’ active and creative engagement with a subject matter. To use digital technologies within pedagogic strategies that foster learners’ transversal skills, deep thinking and creative expression. To open up learning to new, real-world contexts, which involve learners themselves in hands-on activities, scientific investigation or complex problem solving, or in other ways increase learners’ active involvement in complex subject matters."@en ;
    skos:definition "Nutzung von digitalen Medien zur aktiven und kreativen Förderung des Engagements der Lernenden mit einem Thema; Einsatz von digitalen Medien im Rahmen pädagogischer Strategien zur Förderung übertragbarer Fähigkeiten, des tiefgründigen Denkens und kreativen Ausdrucks des Lernenden; Öffnung des Lernens zwecks Schaffung neuer, realer Lernkontexte, dank derer die Lernenden in praktische Aktivitäten, wissenschaftliche Untersuchungen oder komplexe Problemlösungen einbezogen werden, oder anderweitige Erhöhung der aktiven Auseinandersetzung der Lernenden mit komplexen Sachverhalten."@de ;
    skos:broader <5> ;
    skos:exactMatch comeIn:5.3 ;
    skos:inScheme <> .

<6.1> a skos:Concept ;
    skos:prefLabel "Information & media literacy"@en ;
    skos:prefLabel "Informations- und Medienkompetenz"@de ;
    skos:definition "To incorporate learning activities, assignments and assessments which require learners to articulate information needs; to find information and resources in digital environments; to organise, process, analyse and interpret information; and to compare and critically evaluate the credibility and reliability of information and its sources."@en ;
    skos:definition "Einsatz von Lernaktivitäten, Aufgaben und Prüfungen, in denen von den Lernenden erwartet wird, Informationsbedürfnisse zu artikulieren, Informationen und Ressourcen in digitalen Lernumgebungen zu finden, diese zu organisieren, zu verarbeiten, zu analysieren und zu interpretieren sowie die Glaubwürdigkeit und Zuverlässigkeit der Informationen und ihrer Quellen zu vergleichen und kritisch zu bewerten."@de ;
    skos:broader <6> ;
    skos:exactMatch comeIn:6.1 ;
    skos:inScheme <> .

<6.2> a skos:Concept ;
    skos:prefLabel "Communication"@en ;
    skos:prefLabel "Kommunikation und Kollaboration"@de;
    skos:definition "To incorporate learning activities, assignments and assessments which require learners to effectively and responsibly use digital technologies for communication, collaboration and civic participation."@en ;
    skos:definition "Einsatz von Lernaktivitäten, Aufgaben und Prüfungen, in denen von den Lernenden erwartet wird, effektiv und verantwortungsbewusst digitale Medien für Kommunikation, Zusammenarbeit und Bürgerbeteiligung zu nutzen."@de ;
    skos:broader <6> ;
    skos:exactMatch comeIn:6.2 ;
    skos:inScheme <> .

<6.3> a skos:Concept ;
    skos:prefLabel "Content creation"@en ;
    skos:prefLabel "Erstellung digitaler Inhalte"@de ;
    skos:definition "To incorporate learning activities, assignments and assessments which require learners to express themselves through digital means, and to modify and create digital content in different formats. To teach learners how copyright and licenses apply to digital content, how to reference sources and attribute licenses."@en ;
    skos:definition "Einsatz von Lernaktivitäten, Aufgaben und Prüfungen, in denen von den Lernenden erwartet wird, sich mittels digitaler Medien auszudrücken und digitale Inhalte in verschiedenen Formaten zu bearbeiten und zu erstellen; Lernenden vermitteln, wie Urheberrecht und Lizenzen für digitale Inhalte gelten und wie man Quellen zitiert und Lizenzen angibt."@de ;
    skos:broader <6> ;
    skos:exactMatch comeIn:6.3 ;
    skos:inScheme <> .

<6.4> a skos:Concept ;
    skos:prefLabel "Responsible use"@en ;
    skos:prefLabel "Verantwortungsvoller Umgang"@de ;
    skos:definition "To take measures to ensure learners’ physical, psychological and social wellbeing while using digital technologies. To empower learners to manage risks and use digital technologies safely and responsibly."@en ;
    skos:definition "Maßnahmen ergreifen, um das physische, psychische und soziale Wohlergehen der Lernenden bei der Nutzung von digitalen Medien zu gewährleisten; Lernenden eine sichere und verantwortungsvolle Nutzung von digitalen Medien zu ermöglichen und ihnen beibringen, Risiken zu bewältigen."@de ;
    skos:broader <6> ;
    skos:exactMatch comeIn:6.4 ;
    skos:inScheme <> .

<6.5> a skos:Concept ;
    skos:prefLabel "Problem solving"@en ;
    skos:prefLabel "Digitales Problemlösen"@de ;
    skos:definition "To incorporate learning activities, assignments and assessments which require learners to identify and solve technical problems, or to transfer technological knowledge creatively to new situations."@en ;
    skos:definition "Einsatz von Lernaktivitäten, Aufgaben und Prüfungen, in denen von den Lernenden erwartet wird, technische Probleme zu identifizieren und zu lösen oder technisches wissen kreativ auf neue Situationen zu übertragen."@de ;
    skos:broader <6> ;
    skos:exactMatch comeIn:6.5 ;
    skos:inScheme <> .
